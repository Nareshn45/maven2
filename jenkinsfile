pipeline
{
    agent any
    stages
    {
        stage('continuousdownload_Loans')
        {
            steps
            {
                git 'https://github.com/Nareshn45/maven2.git'
            }
        }
        stage('continuousbuild_Loans')
        {
            steps
            {
               sh 'mvn package'
            }
        }
    }
}
